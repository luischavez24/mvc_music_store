@model IEnumerable<Guis.TiendaMusica.Entidades.Tienda.MusicStoreEntities>

@{
    ViewBag.Title = "Browse";
}

<h2>Browsing Genre: @Model.ElementAtOrDefault(1).NameGen</h2>

<ul>
    @foreach (var album in @Model)
    {
        <li>
            @Html.ActionLink(album.Title, "Details", new { id = album.AlbumId})
        </li>
    }
</ul>
